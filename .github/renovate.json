{
  "extends": ["github>product-os/renovate-config"],
  "ignoreDeps": ["postgres"],
  "packageRules": [
    {
      "matchDepTypes": ["devDependencies", "peerDependencies"],
      "matchPackagePatterns": ["@balena/jellyfish-*"],
      "matchUpdateTypes": ["patch"],
      "enabled": false
    },
    {
      "matchPackagePatterns": ["^@json-schema-org"],
      "groupName": "json-schema-org"
    }
  ]
}
