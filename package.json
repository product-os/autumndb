{
  "name": "@balena/jellyfish-core",
  "version": "5.1.26",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-core.git"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "description": "Core library for Jellyfish",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib && npm run check && deplint && depcheck --ignore-bin-package --ignores=shellcheck,@types/jest",
    "lint:fix": "balena-lint --fix lib",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest lib",
    "test:integration": "jest test",
    "test:compose": "docker-compose -f docker-compose.test.yml -f docker-compose.yml up --build --exit-code-from=sut",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "check": "jellycheck",
    "compose": "docker-compose up"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "balena-lint --fix"
    ]
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "@balena/jellyfish-assert": "^1.1.67",
    "@balena/jellyfish-environment": "^5.0.3",
    "@balena/jellyfish-logger": "^3.0.64",
    "@balena/jellyfish-metrics": "^1.0.336",
    "bluebird": "^3.7.2",
    "debounce-promise": "^3.1.2",
    "fast-equals": "^2.0.3",
    "fast-json-patch": "^2.2.1",
    "json-e": "^4.4.1",
    "json-schema": "^0.3.0",
    "json-schema-deref-sync": "^0.14.0",
    "lodash": "^4.17.21",
    "pg-format": "^1.0.4",
    "pg-promise": "^10.11.0",
    "redis": "^3.1.2",
    "redis-mock": "^0.56.3",
    "skhema": "^5.3.4",
    "traverse": "^0.6.6",
    "typed-error": "^3.2.1",
    "uuid": "^8.3.2",
    "uuid-v4-regex": "^1.0.2"
  },
  "devDependencies": {
    "@balena/jellycheck": "^0.1.3",
    "@balena/jellyfish-config": "^1.4.5",
    "@balena/jellyfish-types": "^0.9.9",
    "@balena/lint": "^6.1.1",
    "@json-schema-org/tests": "^1.0.0",
    "@types/bluebird": "^3.5.36",
    "@types/debounce-promise": "^3.1.4",
    "@types/jest": "^27.0.1",
    "@types/json-schema": "^7.0.9",
    "@types/pg-format": "^1.0.2",
    "@types/redis": "^2.8.31",
    "@types/redis-mock": "^0.17.0",
    "@types/traverse": "^0.6.32",
    "@types/uuid": "^8.3.1",
    "depcheck": "^1.4.2",
    "deplint": "^1.1.3",
    "jest": "^27.1.0",
    "lint-staged": "^11.1.2",
    "simple-git-hooks": "^2.6.1",
    "ts-jest": "^27.0.5",
    "typedoc": "^0.21.7",
    "typescript": "^4.4.2"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2021-08-29T02:03:15.226Z"
  }
}
