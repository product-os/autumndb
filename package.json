{
  "name": "autumndb",
  "version": "22.2.88",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/autumndb.git"
  },
  "engines": {
    "node": "^16.0.0"
  },
  "description": "A JSON schema based graph database, based on Postgres",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "bin": "build/cli/index.js",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "npm run types && balena-lint lib test && deplint",
    "lint:fix": "balena-lint --fix lib test",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest test/unit",
    "test:integration": "jest test/integration",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "compose": "docker-compose up",
    "types": "npm run build && ts-node lib/cli/index.ts generate-contract-interfaces && balena-lint --fix lib/types/contracts"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "lib/contracts/*.ts": [
      "npm run types",
      "git add lib/types/contracts/*"
    ],
    "*.{ts,tsx}": [
      "balena-lint --fix"
    ]
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "Apache-2.0",
  "dependencies": {
    "@balena/jellyfish-assert": "^1.2.75",
    "@balena/jellyfish-environment": "^14.0.13",
    "@balena/jellyfish-logger": "^5.1.87",
    "@balena/jellyfish-metrics": "^2.0.193",
    "bluebird": "^3.7.2",
    "commander": "^9.4.0",
    "fast-equals": "^4.0.3",
    "fast-json-patch": "^3.1.1",
    "json-e": "^4.4.3",
    "json-schema": "^0.4.0",
    "json-schema-deref-sync": "^0.14.0",
    "json-schema-to-typescript": "^11.0.2",
    "lodash": "^4.17.21",
    "pg": "^8.8.0",
    "pg-format": "^1.0.4",
    "redis": "4.3.1",
    "redis-mock": "^0.56.3",
    "semver": "^7.3.7",
    "skhema": "^6.0.6",
    "stopword": "^2.0.5",
    "traverse": "^0.6.6",
    "typed-error": "^3.2.1",
    "uuid": "^8.3.2",
    "uuid-v4-regex": "^1.0.2"
  },
  "devDependencies": {
    "@balena/lint": "^6.2.0",
    "@json-schema-org/tests": "^2.0.0",
    "@types/bluebird": "^3.5.37",
    "@types/jest": "^29.0.3",
    "@types/json-schema": "^7.0.11",
    "@types/node": "^18.0.6",
    "@types/pg": "^8.6.5",
    "@types/pg-format": "^1.0.2",
    "@types/redis-mock": "^0.17.1",
    "@types/semver": "^7.3.12",
    "@types/traverse": "^0.6.32",
    "@types/uuid": "^8.3.4",
    "deplint": "^1.1.3",
    "jest": "^29.0.3",
    "lint-staged": "^13.0.3",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.8.0",
    "ts-jest": "^29.0.2",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.15",
    "typescript": "^4.8.3"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2022-09-27T14:47:34.013Z"
  }
}
