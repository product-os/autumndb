{
  "name": "@balena/jellyfish-core",
  "version": "1.3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-core.git"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "description": "Core library for Jellyfish",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{js}"
    ],
    "modules": [
      "lib"
    ]
  },
  "scripts": {
    "test": "npm run doc && catch-uncommitted --skip-node-versionbot-changes --exclude=VERSION && make lint && make test-unit",
    "doc": "jsdoc2md --template doc/README.hbs 'lib/**/*.js' > README.md",
    "postinstall": "if [ -z \"$CI\" ] && [ -d \".git\" ]; then husky install; fi"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "@balena/jellyfish-assert": "^1.0.53",
    "@balena/jellyfish-environment": "^2.4.1",
    "@balena/jellyfish-logger": "1.0.1",
    "@balena/jellyfish-metrics": "0.1.55",
    "@balena/jellyfish-uuid": "^1.0.57",
    "bluebird": "^3.7.2",
    "debounce-promise": "^3.1.2",
    "fast-equals": "^2.0.0",
    "fast-json-patch": "^2.2.1",
    "json-e": "^4.3.0",
    "json-schema-deref-sync": "^0.14.0",
    "lodash": "^4.17.20",
    "pg-format": "^1.0.4",
    "pg-promise": "^10.9.2",
    "redis": "^3.0.2",
    "redis-mock": "^0.56.3",
    "skhema": "^5.3.3",
    "traverse": "^0.6.6",
    "typed-errors": "^1.1.0"
  },
  "devDependencies": {
    "@balena/ci-task-runner": "^0.2.94",
    "@balena/jellycheck": "^0.1.1",
    "@json-schema-org/tests": "^1.0.0",
    "ava": "^3.15.0",
    "catch-uncommitted": "^1.6.2",
    "depcheck": "^1.3.1",
    "deplint": "^1.1.3",
    "eslint": "^7.19.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^31.6.0",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^5.0.8",
    "jsdoc-to-markdown": "^6.0.1",
    "lint-staged": "^10.5.3",
    "uuid": "^8.3.2"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "npm run doc"
    ]
  }
}
